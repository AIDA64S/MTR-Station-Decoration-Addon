{
  "multipart": [
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "msd:block/electric_pole"
      }
    },
    {
      "when": {
        "north": "low"
      },
      "apply": {
        "model": "msd:block/electric_pole_side"
      }
    },
    {
      "when": {
        "east": "low"
      },
      "apply": {
        "model": "msd:block/electric_pole_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": "low"
      },
      "apply": {
        "model": "msd:block/electric_pole_side",
        "y": 180
      }
    },
    {
      "when": {
        "west": "low"
      },
      "apply": {
        "model": "msd:block/electric_pole_side",
        "y": 270
      }
    },
    {
      "when": {
        "north": "tall"
      },
      "apply": {
        "model": "msd:block/electric_pole_side"
      }
    },
    {
      "when": {
        "east": "tall"
      },
      "apply": {
        "model": "msd:block/electric_pole_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": "tall"
      },
      "apply": {
        "model": "msd:block/electric_pole_side",
        "y": 180
      }
    },
    {
      "when": {
        "west": "tall"
      },
      "apply": {
        "model": "msd:block/electric_pole_side",
        "y": 270
      }
    }
  ]
}